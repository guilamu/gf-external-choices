#. Plugin Name of the plugin
msgid "Gravity Forms External Choices"
msgstr "Gravity Forms Choix Externes"

#. Description of the plugin
msgid "Populate Gravity Forms Multiple Choice fields from external CSV, JSON, or XLSX data sources."
msgstr "Remplir les champs à choix multiples de Gravity Forms à partir de sources de données externes CSV, JSON ou XLSX."

#: includes/class-field-settings.php
msgid "External Choices"
msgstr "Choix externes"

#: includes/class-field-settings.php
msgid "Populate choices from an external CSV or JSON file. The file can be from a URL or uploaded to your Media Library."
msgstr "Remplir les choix à partir d'un fichier CSV ou JSON externe. Le fichier peut provenir d'une URL ou être téléchargé dans votre bibliothèque de médias."

#: includes/class-field-settings.php
msgid "Source Type"
msgstr "Type de source"

#: includes/class-field-settings.php
msgid "Manual (Default)"
msgstr "Manuel (Par défaut)"

#: includes/class-field-settings.php
msgid "External URL"
msgstr "URL externe"

#: includes/class-field-settings.php
msgid "Media Library"
msgstr "Bibliothèque de médias"

#: includes/class-field-settings.php
msgid "URL"
msgstr "URL"

#: includes/class-field-settings.php
msgid "Media File"
msgstr "Fichier média"

#: includes/class-field-settings.php
msgid "No file selected"
msgstr "Aucun fichier sélectionné"

#: includes/class-field-settings.php
msgid "Select File"
msgstr "Sélectionner un fichier"

#: includes/class-field-settings.php
msgid "Label Column/Property"
msgstr "Colonne/Propriété du libellé"

#: includes/class-field-settings.php
msgid "-- Select Column --"
msgstr "-- Sélectionner une colonne --"

#: includes/class-field-settings.php
msgid "Value Column/Property"
msgstr "Colonne/Propriété de la valeur"

#: includes/class-field-settings.php
msgid "Refresh Frequency"
msgstr "Fréquence de rafraîchissement"

#: includes/class-field-settings.php
msgid "Hourly"
msgstr "Toutes les heures"

#: includes/class-field-settings.php
msgid "Daily"
msgstr "Quotidien"

#: includes/class-field-settings.php
msgid "Weekly"
msgstr "Hebdomadaire"

#: includes/class-field-settings.php
msgid "Force Refresh"
msgstr "Forcer le rafraîchissement"

#: includes/class-field-settings.php
msgid "File selected (ID: "
msgstr "Fichier sélectionné (ID : "

#: includes/class-data-fetcher.php
msgid "URL is empty."
msgstr "L'URL est vide."

#: includes/class-data-fetcher.php
#, php-format
msgid "Failed to fetch URL: %s"
msgstr "Échec de la récupération de l'URL : %s"

#: includes/class-data-fetcher.php
#, php-format
msgid "HTTP error: %d"
msgstr "Erreur HTTP : %d"

#: includes/class-data-fetcher.php
#, php-format
msgid "File exceeds maximum size of %s."
msgstr "Le fichier dépasse la taille maximale de %s."

#: includes/class-data-fetcher.php
msgid "Media file not found."
msgstr "Fichier média introuvable."

#: includes/class-data-fetcher.php
msgid "Failed to read media file."
msgstr "Échec de la lecture du fichier média."

#: includes/class-csv-parser.php
msgid "CSV data is empty."
msgstr "Les données CSV sont vides."

#: includes/class-csv-parser.php
msgid "CSV must have a header row and at least one data row."
msgstr "Le CSV doit avoir une ligne d'en-tête et au moins une ligne de données."

#: includes/class-csv-parser.php
msgid "No valid choices found in CSV."
msgstr "Aucun choix valide trouvé dans le CSV."

#: includes/class-json-parser.php
msgid "JSON data is empty."
msgstr "Les données JSON sont vides."

#: includes/class-json-parser.php
#, php-format
msgid "JSON parse error: %s"
msgstr "Erreur d'analyse JSON : %s"

#: includes/class-json-parser.php
msgid "JSON must be an array at the root level."
msgstr "Le JSON doit être un tableau au niveau racine."

#: includes/class-json-parser.php
msgid "JSON array is empty."
msgstr "Le tableau JSON est vide."

#: includes/class-json-parser.php
msgid "JSON must contain flat objects only. Nested objects are not supported in this version."
msgstr "Le JSON ne doit contenir que des objets plats. Les objets imbriqués ne sont pas pris en charge dans cette version."

#: includes/class-json-parser.php
msgid "No valid choices found in JSON."
msgstr "Aucun choix valide trouvé dans le JSON."

#: includes/class-json-parser.php
msgid "JSON must be a non-empty array."
msgstr "Le JSON doit être un tableau non vide."

#: includes/class-json-parser.php
msgid "JSON array must contain objects."
msgstr "Le tableau JSON doit contenir des objets."

#: includes/class-xlsx-parser.php
msgid "XLSX must have a header row and at least one data row."
msgstr "Le XLSX doit avoir une ligne d'en-tête et au moins une ligne de données."

#: includes/class-xlsx-parser.php
msgid "No valid choices found in XLSX."
msgstr "Aucun choix valide trouvé dans le XLSX."

#: includes/class-xlsx-parser.php
msgid "XLSX file is empty."
msgstr "Le fichier XLSX est vide."

#: includes/class-xlsx-parser.php
msgid "PHP ZipArchive extension is required for XLSX support."
msgstr "L'extension PHP ZipArchive est requise pour la prise en charge des fichiers XLSX."

#: includes/class-xlsx-parser.php
msgid "XLSX file not found."
msgstr "Fichier XLSX introuvable."

#: includes/class-xlsx-parser.php
msgid "Failed to open XLSX file."
msgstr "Échec de l'ouverture du fichier XLSX."

#: includes/class-xlsx-parser.php
msgid "No worksheet found in XLSX file."
msgstr "Aucune feuille de calcul trouvée dans le fichier XLSX."

#: includes/class-xlsx-parser.php
msgid "Failed to parse worksheet XML."
msgstr "Échec de l'analyse du XML de la feuille de calcul."

#: includes/class-github-updater.php
msgid "Unable to rename the update folder. Please retry or update manually."
msgstr "Impossible de renommer le dossier de mise à jour. Veuillez réessayer ou mettre à jour manuellement."
